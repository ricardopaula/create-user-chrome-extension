{
  "name": "Create User",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Put user data to inputs",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Create User",
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}

